<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kieferl.hallowelt.app">

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:icon="@mipmap/ic_launcher"
        android:theme="@style/AppTheme">

        <activity
            android:name=".ListenUebersichtMainActivity"
            android:label="@string/title_activity_main"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TaschenrechnerActivity"
            android:label="ListenHQ"/>
        <activity android:name=".LeereListe"
            android:label="Liste"
            android:screenOrientation="portrait"
            android:parentActivityName=".ListenUebersichtMainActivity" />
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>
