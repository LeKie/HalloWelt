<?xml version="1.0" encoding="utf-8"?>

<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".ListenUebersichtFragment">

    <ListView
        android:id="@+id/listview_leereliste"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent" >

        <TextView
            android:id="@+id/listenName"
            android:layout_width="fill_parent"
            android:layout_height="70dp"
            android:textSize="20pt"
            android:paddingTop="10dp"
            android:gravity="left"
            />

    <EditText
        android:id="@+id/listenelement1"
        android:layout_width="fill_parent"
        android:layout_height="50dp"
        android:textSize="14pt"
        android:paddingTop="10dp"
        android:gravity="left"
        />
        <EditText
            android:id="@+id/listenelement2"
            android:layout_width="fill_parent"
            android:layout_height="50dp"
            android:textSize="14pt"
            android:paddingTop="10dp"
            android:gravity="left"
            />
        <EditText
            android:id="@+id/listenelement3"
            android:layout_width="fill_parent"
            android:layout_height="50dp"
            android:textSize="14pt"
            android:paddingTop="10dp"
            android:gravity="left"
            />
        <EditText
            android:id="@+id/listenelement4"
            android:layout_width="fill_parent"
            android:layout_height="50dp"
            android:textSize="14pt"
            android:paddingTop="10dp"
            android:gravity="left"
            />
        <EditText
            android:id="@+id/listenelement5"
            android:layout_width="fill_parent"
            android:layout_height="50dp"
            android:textSize="14pt"
            android:paddingTop="10dp"
            android:gravity="left"
            />
        <EditText
            android:id="@+id/listenelement6"
            android:layout_width="fill_parent"
            android:layout_height="50dp"
            android:textSize="14pt"
            android:paddingTop="10dp"
            android:gravity="left"
            />
        <EditText
            android:id="@+id/listenelement7"
            android:layout_width="fill_parent"
            android:layout_height="50dp"
            android:textSize="14pt"
            android:paddingTop="10dp"
            android:gravity="left"
            />
        <EditText
            android:id="@+id/listenelement8"
            android:layout_width="fill_parent"
            android:layout_height="50dp"
            android:textSize="14pt"
            android:paddingTop="10dp"
            android:gravity="left"
            />
        <EditText
            android:id="@+id/listenelement9"
            android:layout_width="fill_parent"
            android:layout_height="50dp"
            android:textSize="14pt"
            android:paddingTop="10dp"
            android:gravity="left"
            />
        <EditText
            android:id="@+id/listenelement10"
            android:layout_width="fill_parent"
            android:layout_height="50dp"
            android:textSize="14pt"
            android:paddingTop="10dp"
            android:gravity="left"
            />
    </LinearLayout>
</FrameLayout>